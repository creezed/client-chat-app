{
  "trailingComma": "none",
  "singleQuote": false,
  "tabWidth": 4,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "semi": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^components/(.*)$",
    "^utils/(.*)$",
    "(.scss)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}

